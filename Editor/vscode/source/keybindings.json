// 将键绑定放在此文件中以覆盖默认值
[
  {
    "key": "ctrl+k ctrl+b",
    "command": "extension.openInDefaultBrowser"
  },
  {
    "key": "alt+b",
    "command": "-extension.openInDefaultBrowser"
  },
  {
    "key": "ctrl+k ctrl+enter",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "f12",
    "command": "-editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
  }
]
